{
    "version": 2,
    "builds": [
      {
        "src": "frontend/package.json",
        "use": "@vercel/node"
      },
      {
        "src": "backend/package.json",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/",
        "dest": "/backend/app.js"
      },
      {
        "src": "/",
        "dest": "/frontend/index.js"
      }
    ]
  }
  